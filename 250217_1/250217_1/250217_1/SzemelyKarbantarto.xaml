<Window x:Class="_250217_1.SzemelyKarbantarto"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_250217_1"
        mc:Ignorable="d"
        Title="SzemelyKarbantarto" Height="470.397" Width="381.87">
    <StackPanel>
        <Label Height="40" Content="Név" Background="GreenYellow" />
        <TextBox x:Name="tb_nev" Height="40" FontSize="30" Text="{Binding Nev, UpdateSourceTrigger=Explicit}"/>
        <Label Height="40" Content="Telefonszám" Background="GreenYellow" />
        <TextBox x:Name="tb_tel" Height="40" FontSize="30" Text="{Binding Telefonszam, UpdateSourceTrigger=Explicit}"/>
        <Label Height="40" Content="Étetkor" Background="GreenYellow" />
        <TextBox x:Name="tb_eletkor" Height="40" FontSize="30" Text="{Binding Eletkor, UpdateSourceTrigger=Explicit}"/>
        <Button x:Name="btn_mentes" Content="Mentés" Height="40" Margin="10,80,10,0" FontSize="26" Click="btn_mentes_Click"/>
    </StackPanel>
</Window>
